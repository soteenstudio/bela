{
  "name": "@soteen/bela",
  "version": "0.0.4-dev",
  "description": "This is machine learning using BELA.",
  "type": "module",
  "main": "./dist/index.min.cjs",
  "module": "./dist/index.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.min.cjs",
      "import": "./dist/index.min.mjs"
    }
  },
  "typings": "./dist/types/typings.d.ts",
  "scripts": {
    "ready": "npm pack",
    "build": "node ./esbuild.config.js",
    "build:bundling-dts": "dts-bundle --name '@soteen/bela' --main typings/index.d.ts --baseDir typings/ --out ../dist/types/typings.d.ts --verbose",
    "typecheck": "tsc",
    "checkcount": "find src -type f | wc -l",
    "jest": "node ./node_modules/jest/bin/jest.js",
    "test:training": "node ./tests/script/training.js",
    "test:finetune": "node ./tests/script/finetune.js",
    "test:inference": "node ./tests/script/inference.js",
    "test:move": "node ./tests/script/move.js",
    "test:read": "node ./tests/script/read.js"
  },
  "keywords": [
    "machine",
    "learning",
    "bela"
  ],
  "author": "SoTeen Studio",
  "license": "MIT",
  "devDependencies": {
    "@soteen/bela": "^0.0.2-dev",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.5",
    "i": "^0.3.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "chalk": "^5.4.1"
  }
}
